                                    <border:SfBorder Grid.Row="8" Grid.Column="2" Grid.ColumnSpan="3"
                                                             BorderColor="#319C8A"
                                                             IsVisible="{Binding Path=ProgressProxyModel.MeditationProgress.IsActualLightFood}"
                                                             Style="{StaticResource progress_Line}" />
                                    <border:SfBorder Grid.Row="10" Grid.Column="2" Grid.ColumnSpan="3"
                                                             BorderColor="#98CEC5"
                                                             IsVisible="{Binding Path=ProgressProxyModel.MeditationProgress.IsActualVeryLightFood}"
                                                             Style="{StaticResource progress_Line}" />

                                    <!--Target Line -->
                                    <border:SfBorder Grid.Row="2" Grid.Column="0" Grid.ColumnSpan="3"
                                                             BorderColor="#FF5869"
                                                             IsVisible="{Binding Path=ProgressProxyModel.MeditationProgress.IsVeryHeavyFood}"
                                                             Style="{StaticResource progress_Line}" />
                                    <border:SfBorder Grid.Row="4" Grid.Column="0" Grid.ColumnSpan="3"
                                                             BorderColor="#ffacb4"
                                                             IsVisible="{Binding Path=ProgressProxyModel.MeditationProgress.IsHeavyFood}"
                                                             Style="{StaticResource progress_Line}" />
                                    <border:SfBorder Grid.Row="6" Grid.Column="0" Grid.ColumnSpan="3"
                                                             BorderColor="#FFC658"
                                                             IsVisible="{Binding Path=ProgressProxyModel.MeditationProgress.IsMediumFood}"
                                                             Style="{StaticResource progress_Line}" />
                                    <border:SfBorder Grid.Row="8" Grid.Column="0" Grid.ColumnSpan="3"
                                                             BorderColor="#319C8A"
                                                             IsVisible="{Binding Path=ProgressProxyModel.MeditationProgress.IsLightFood}"
                                                             Style="{StaticResource progress_Line}" />
                                    <border:SfBorder Grid.Row="10" Grid.Column="0" Grid.ColumnSpan="3"
                                                             IsVisible="{Binding Path=ProgressProxyModel.MeditationProgress.IsVeryLightFood}"
                                                             BorderColor="#98CEC5"
                                                             Style="{StaticResource progress_Line}" />

                                    <border:SfBorder Grid.Row="1" Grid.RowSpan="2" Grid.Column="2" Style="{StaticResource progress_VeryHeavy}">
                                        <Label Text="30 mins"  Style="{StaticResource progress_label}" />
                                    </border:SfBorder>
                                    <border:SfBorder Grid.Row="3" Grid.RowSpan="2" Grid.Column="2" Style="{StaticResource progress_Heavy}">
                                        <Label Text="25 mins" Style="{StaticResource progress_label}" />
                                    </border:SfBorder>
                                    <border:SfBorder Grid.Row="5" Grid.RowSpan="2" Grid.Column="2" Style="{StaticResource progress_Medium}">
                                        <Label Text="20 mins" Style="{StaticResource progress_label}" />
                                    </border:SfBorder>
                                    <border:SfBorder Grid.Row="7" Grid.RowSpan="2" Grid.Column="2" Style="{StaticResource progress_Light}">
                                        <Label Text="15 mins" Style="{StaticResource progress_label}" />
                                    </border:SfBorder>
                                    <border:SfBorder Grid.Row="9" Grid.RowSpan="2" Grid.Column="2" Style="{StaticResource progress_VeryLight}">
                                        <Label Text="10 mins" Style="{StaticResource progress_label}" />
                                    </border:SfBorder>
                                </Grid>
                                <Label Grid.Row="2" Text="Recommended per day " Style="{StaticResource ProgressDaily_label}" />
                            </Grid>

                        </border:SfBorder>
                    </Grid>
                </Grid>
            </ScrollView>
            <StackLayout Grid.Row="1" BackgroundColor="Transparent">
                <buttons:SfButton Text="Next" IsEnabled="{Binding IsBusy, Converter={StaticResource  InverseBoolConverter} }"
                                  CornerRadius="30"
                                  HorizontalOptions="Center"
                                  Margin="10"
                                  BackgroundColor="#319C8A"
                                  TextColor="#FFF"
                                  WidthRequest="140"
                                  HeightRequest="40"
                                  Command="{Binding WizardFinishSetupSaveDataCommand}" />
            </StackLayout>
        </Grid>

    </ContentPage.Content>
</base:ContentPageBase>