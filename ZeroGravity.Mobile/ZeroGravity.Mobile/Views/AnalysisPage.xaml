                                                    <border:SfBorder Grid.Row="6" Grid.Column="0" Grid.ColumnSpan="3"
                                                             BorderColor="#FFC658"
                                                             IsVisible="{Binding Path=ProgressProxyModel.MeditationProgress.IsMediumFood}"
